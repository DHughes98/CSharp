@{
    ViewData["Title"] = "Home Page";
}
@model List<Dish>
<div>
    <h1>Welcome to CRUDelicious</h1>
    <div>
        <a href="/">Home</a>
        <a href="/dishes/new">Add New Dish</a>
    </div>
    <h1>Check out some recent dishes</h1>
    </div>
    @foreach (Dish d in Model)
    {
        <h3><a href="dishes/@d.DishID">@d.Name</a> by @d.Chef</h3>
    }